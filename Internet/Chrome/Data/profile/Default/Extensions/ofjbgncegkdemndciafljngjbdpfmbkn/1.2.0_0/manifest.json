{
   "content_scripts": [ {
      "css": [ "content.css" ],
      "matches": [ "https://github.com/*/*", "https://gist.github.com/*/*", "https://raw.githubusercontent.com/*/*" ],
      "run_at": "document_end"
   } ],
   "description": "Make tab indented code more readable by forcing the tab size to 4 instead of 8",
   "homepage_url": "https://github.com/sindresorhus/tab-size-on-github",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "36",
   "name": "Tab Size on GitHub",
   "permissions": [ "https://github.com/*/*", "https://gist.github.com/*/*", "https://raw.githubusercontent.com/*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0"
}
