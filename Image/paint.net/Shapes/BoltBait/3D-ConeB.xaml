<ps:SimpleGeometryShape xmlns="clr-namespace:PaintDotNet.UI.Media;assembly=PaintDotNet.Framework"
                        xmlns:ps="clr-namespace:PaintDotNet.Shapes;assembly=PaintDotNet.Framework"
                        DisplayName="Cone">
	<PathGeometry FillRule="Nonzero">
		<PathFigure IsClosed="True" IsFilled="True" StartPoint="0,50">
			<ArcSegment Point="200,50" Size="15,5" IsLargeArc="True" SweepDirection="Clockwise" RotationAngle="0"/>
			<LineSegment Point="200,53" />
			<LineSegment Point="100,250" />
			<LineSegment Point="0,53" />
		</PathFigure>
		<PathFigure IsClosed="False" IsFilled="False" StartPoint="0,50">
			<ArcSegment Point="200,50" Size="15,5" IsLargeArc="True" SweepDirection="CounterClockwise" RotationAngle="0"/>
		</PathFigure>
	</PathGeometry>
</ps:SimpleGeometryShape>
