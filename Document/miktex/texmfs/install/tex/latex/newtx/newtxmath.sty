\newread\tx@su

\def\readsufile#1{%
  \tx@tA={}
  \tx@tB={}
  \openin\tx@su=#1
  \loop\unless\ifeof\tx@su
  \read\tx@su to \tx@line
  \if\tx@line\par\else
    \expandafter\tx@addrec\tx@line
  \fi
  \repeat
}
\def\tx@addrec#1#2{%
\ifiscseq{#1}{\tx@tA\expandafter{\the\tx@tA\if}}{\tx@tA\expandafter{\the\tx@tA\if}}
\tx@tA\expandafter{\the\tx@tA\next #1\mkern #2 mu\else}
\tx@tB\expandafter{\the\tx@tB\fi}}

