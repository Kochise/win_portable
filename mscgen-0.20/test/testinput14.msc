msc
{
  a, b;

  --- [ label = "1" ];
  a->b;
  --- [ label = "1\n2" ];
  a->b;
  --- [ label = "1\n2\n3" ];
  a->b;
  --- [ label = "1\n2\n3\n4" ];
  a->b;
  --- [ label = "a\nmuch longer" ];
  --- [ label = "muchlonger\nb" ];
  a->b;

  --- [label = "a\nb\nc" ];

  a->b [label = "a\nb" ];
  a->b [label = "a\nb\nc" ];

  a->b;

}

