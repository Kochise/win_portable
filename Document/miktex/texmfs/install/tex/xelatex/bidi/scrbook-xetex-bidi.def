        \ifnum\@listdepth=\@ne\advance
          \if@RTL
            \rightmargin #2\relax
          \else
            \leftmargin #2\relax
          \fi
        \fi
      }%
      \parindent #3\noindent
      \IfArgIsEmpty{#1}{}{%
        \hskip \dimexpr #1-#2\relax
