  \ifx #2\protect
    \expandafter\CJK@XXp\expandafter#1
  \else
    \expandafter\ifx\csname u8:\string #1\string #2\endcsname \relax
      \csname CJK@\number `#1\endcsname{`#2}
      \expandafter\CJK@ignorespaces
    \else
      \csname u8:\string #1\string #2\endcsname
    \fi
  \fi}
