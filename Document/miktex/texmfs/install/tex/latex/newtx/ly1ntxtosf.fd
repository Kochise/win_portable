\DeclareFontShape{LY1}{ntxtosf}{bx}{sc}{<->ssub * ntxtosf/b/sc}{}
\DeclareFontShape{LY1}{ntxtosf}{bx}{n}{<->ssub * ntxtosf/b/n}{}
\DeclareFontShape{LY1}{ntxtosf}{bx}{scit}{<->ssub * ntxtosf/b/scit}{}
